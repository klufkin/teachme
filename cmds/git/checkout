#!/usr/bin/env bash

# The command's name
echo -e 'git checkout'

# the command's description
echo "description here"

echo "------------"

# the command's syntax
echo "Syntax:"
echo "git checkout -b new_branch"
echo -e "\t-b: Tells git to run \033[4mgit branch new_branch\033[0m before checkout."
